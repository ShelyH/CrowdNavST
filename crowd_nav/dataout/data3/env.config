
[env]
time_limit = 25
time_step = 0.25
val_size = 100
test_size = 500
randomize_attributes = false
ros_visualize=true
multiagent_training=true

[reward]
success_reward = 100
collision_penalty = -50
discomfort_dist = 0.4
discomfort_penalty_factor = 5


[sim]
train_val_sim = circle_crossing
test_sim = circle_crossing
%test_sim = mixed

square_width = 10
circle_radius = 4
human_num = 5


[humans]
visible = true
policy = orca
radius = 0.4
v_pref = 1
sensor = coordinates


[robot]
visible = false
policy = none
radius = 0.5
v_pref = 1
sensor = coordinates
